	<script>
    function performSearch(event) {
        event.preventDefault(); // Prevent the form from submitting normally

        // Get the search term entered in the input field
        var searchTerm = document.getElementById("search").value;

        // Convert the search term to a regular expression with 'g' flag for global search
        var searchRegex = new RegExp(searchTerm, "gi");

        // Get all spans with underline and remove the underline
        var underlineSpans = document.querySelectorAll("span.underline");
        underlineSpans.forEach(span => {
            span.outerHTML = span.textContent; // Replace the span with its content (removing the span element)
        });

        // Get all text nodes in the HTML page
        var allTextNodes = document.createTreeWalker(document.body, NodeFilter.SHOW_TEXT, null, false);

        // Variable to store if the search term is found
        var isTermFound = false;

        // Loop through all text nodes and underline the matches and scroll to the first match
        while (allTextNodes.nextNode()) {
            var textNode = allTextNodes.currentNode;
            var textContent = textNode.textContent;

            // Check if the text content contains the search term
            if (textContent.match(searchRegex)) {
                isTermFound = true;

                // Wrap the matched text in a span with an underline class
                var spanNode = document.createElement("span");
                spanNode.className = "underline";
                spanNode.innerHTML = textContent.replace(searchRegex, match => {
                    return `<u>${match}</u>`;
                });

                // Replace the original text node with the new underlined span
                textNode.parentNode.replaceChild(spanNode, textNode);

                // Scroll to the first occurrence of the search term
                spanNode.scrollIntoView();
                break;
            }
        }

        // If the search term is not found, show an alert
        if (!isTermFound) {
            alert("Search term not found!");
        }
    }
